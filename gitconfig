[alias]
st  = status
pom = push origin master
pr  = pull --rebase
ca  = commit --amend
lp = log -p
d = diff
l = log
lg = log --graph --oneline --decorate --color --all

[color]
branch = auto
diff = auto
status = auto

[color "branch"]
current = yellow reverse
local = yellow
remote = green

[color "diff"]
meta = yellow bold
frag = magenta bold
old = red bold
new = green bold

[remote "<nickname>"]
url = <url>
fetch = <refspec>

[color "status"]
added = yellow
changed = green
untracked = cyan
